{
	"multipart": [
	{	"apply": { "model": "engineerstech:block/cable" }, "uvlock": true},
    {	"when": {"north": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"uvlock": true
    	}
    },
    {	"when": {"east": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"y": 90,
    		"uvlock": true
    	}
    },{	"when": {"south": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"y": 180,
    		"uvlock": true
    	}
    },{	"when": {"west": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"y": 270,
    		"uvlock": true
    	}
    },{	"when": {"up": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"x": 270,
    		"uvlock": true
    	}
    },{	"when": {"down": "true" },
    	"apply": { 
    		"model": "engineerstech:block/cable_extend",
    		"x": 90,
    		"uvlock": true
    	}
    }
]}